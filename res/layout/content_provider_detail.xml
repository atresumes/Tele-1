                <TextView app:id="@+id/txt_city_dash" app:layout_width="wrap_content" app:layout_height="wrap_content" />
                <TextView app:id="@+id/txt_state_dash" app:layout_width="wrap_content" app:layout_height="wrap_content" />
                <TextView app:id="@+id/txt_country_dash" app:layout_width="wrap_content" app:layout_height="wrap_content" />
            </LinearLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>
