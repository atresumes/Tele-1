                <TextView app:textColor="@color/secondary_text" app:id="@+id/txt_rates" app:layout_width="wrap_content" app:layout_height="wrap_content" app:layout_marginTop="4dp" />
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <TextView app:textColor="@color/primary_text" app:layout_width="wrap_content" app:layout_height="wrap_content" app:layout_marginTop="@dimen/activity_vertical_margin" app:text="@string/credentials" app:textAllCaps="true" />
        <android.support.v7.widget.CardView app:padding="8dp" app:layout_width="match_parent" app:layout_height="wrap_content" app:layout_marginTop="4dp" app:elevation="@dimen/cardview_default_elevation" app:cardCornerRadius="4dp">
            <LinearLayout app:orientation="vertical" app:padding="8dp" app:layout_width="match_parent" app:layout_height="wrap_content">
                <TextView app:textSize="15sp" app:textColor="@color/primary_text" app:layout_width="wrap_content" app:layout_height="wrap_content" app:layout_marginTop="4dp" app:text="@string/licenses" />
                <TextView app:id="@+id/txt_licenses" app:layout_width="wrap_content" app:layout_height="wrap_content" />
                <TextView app:textSize="15sp" app:textColor="@color/primary_text" app:layout_width="wrap_content" app:layout_height="wrap_content" app:layout_marginTop="6dp" app:text="@string/education" />
                <TextView app:id="@+id/txt_education" app:layout_width="wrap_content" app:layout_height="wrap_content" />
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <TextView app:textColor="@color/primary_text" app:layout_width="wrap_content" app:layout_height="wrap_content" app:layout_marginTop="@dimen/activity_vertical_margin" app:text="@string/address" app:textAllCaps="true" />
